{
  "manifest_version": 3,
  "name": "Viking Extensions",
  "version": "1.0.0",
  "description": "Copies AWS credentials and prompts to save locally",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Viking Extensions"
  },
  "permissions": ["activeTab", "browsingData", "scripting", "tabs"]
}
